    flags+=("--ref=")
    two_word_flags+=("--ref")
    two_word_flags+=("-r")
    local_nonpersistent_flags+=("--ref")
    local_nonpersistent_flags+=("--ref=")
    local_nonpersistent_flags+=("-r")
    flags+=("--rm")
    local_nonpersistent_flags+=("--rm")
    flags+=("--run")
    local_nonpersistent_flags+=("--run")
    flags+=("--runtime-artifact=")
    two_word_flags+=("--runtime-artifact")
    two_word_flags+=("-a")
    local_nonpersistent_flags+=("--runtime-artifact")
    local_nonpersistent_flags+=("--runtime-artifact=")
    local_nonpersistent_flags+=("-a")
    flags+=("--runtime-image=")
    two_word_flags+=("--runtime-image")
    local_nonpersistent_flags+=("--runtime-image")
    local_nonpersistent_flags+=("--runtime-image=")
    flags+=("--runtime-pull-policy=")
    two_word_flags+=("--runtime-pull-policy")
    local_nonpersistent_flags+=("--runtime-pull-policy")
    local_nonpersistent_flags+=("--runtime-pull-policy=")
    flags+=("--save-temp-dir")
    local_nonpersistent_flags+=("--save-temp-dir")
    flags+=("--scripts=")
    two_word_flags+=("--scripts")
    local_nonpersistent_flags+=("--scripts")
    local_nonpersistent_flags+=("--scripts=")
    flags+=("--scripts-url=")
    two_word_flags+=("--scripts-url")
    two_word_flags+=("-s")
    local_nonpersistent_flags+=("--scripts-url")
    local_nonpersistent_flags+=("--scripts-url=")
    local_nonpersistent_flags+=("-s")
    flags+=("--use-config")
    local_nonpersistent_flags+=("--use-config")
    flags+=("--volume=")
    two_word_flags+=("--volume")
    two_word_flags+=("-v")
    local_nonpersistent_flags+=("--volume")
    local_nonpersistent_flags+=("--volume=")
    local_nonpersistent_flags+=("-v")
    flags+=("--ca=")
    two_word_flags+=("--ca")
    flags+=("--cert=")
    two_word_flags+=("--cert")
    flags+=("--key=")
    two_word_flags+=("--key")
    flags+=("--loglevel=")
    two_word_flags+=("--loglevel")
    flags+=("--tls")
    flags+=("--tlsverify")
    flags+=("--url=")
    two_word_flags+=("--url")
    two_word_flags+=("-U")

    must_have_one_flag=()
    must_have_one_noun=()
    noun_aliases=()
}

_s2i_completion()
{
    last_command="s2i_completion"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--help")
    flags+=("-h")
    local_nonpersistent_flags+=("--help")
    local_nonpersistent_flags+=("-h")
    flags+=("--ca=")
    two_word_flags+=("--ca")
    flags+=("--cert=")
    two_word_flags+=("--cert")
    flags+=("--key=")
    two_word_flags+=("--key")
    flags+=("--loglevel=")
    two_word_flags+=("--loglevel")
    flags+=("--tls")
    flags+=("--tlsverify")
    flags+=("--url=")
    two_word_flags+=("--url")
    two_word_flags+=("-U")

    must_have_one_flag=()
    must_have_one_noun=()
    must_have_one_noun+=("bash")
    must_have_one_noun+=("zsh")
    noun_aliases=()
}

