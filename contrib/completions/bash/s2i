    flags+=("--network=")
    two_word_flags+=("--network")
    local_nonpersistent_flags+=("--network")
    local_nonpersistent_flags+=("--network=")
    flags+=("--pull-policy=")
    two_word_flags+=("--pull-policy")
    two_word_flags+=("-p")
