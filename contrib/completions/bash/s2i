_s2i_help()
{
    last_command="s2i_help"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--ca=")
    two_word_flags+=("--ca")
    flags+=("--cert=")
    two_word_flags+=("--cert")
    flags+=("--key=")
    two_word_flags+=("--key")
    flags+=("--loglevel=")
    two_word_flags+=("--loglevel")
    flags+=("--tls")
    flags+=("--tlsverify")
    flags+=("--url=")
    two_word_flags+=("--url")
    two_word_flags+=("-U")

    must_have_one_flag=()
    must_have_one_noun=()
    has_completion_function=1
    noun_aliases=()
}

